<?xml version="1.0" ?><drawMathFn curveRes="20" descr="https://en.wikipedia.org/wiki/Butterfly_curve_(transcendental)" name="Butterfly" type="PARAMETRIC"><functions tMapTo="HORIZONTAL" tScaleFact="2.0" tStartVal="0.0"><paramFn1 equation="B * (sin(D * t) * (2.718 ** cos(t) - 2 * cos(C * t) - sin(E * t / 12) ** 5))"/><paramFn2 equation="A * (cos(D * t) * (2.718 ** cos(t) - 2 * cos(C * t) - sin(E * t / 12) ** 5))"/><constant_A incr="0.1" value="0.5"/><constant_B incr="0.1" value="0.5"/><constant_C incr="0.1" value="4.0"/><constant_D incr="0.1" value="1.0"/><constant_E incr="0.1" value="1.0"/><constant_F incr="0.1" value="1.0"/></functions></drawMathFn>